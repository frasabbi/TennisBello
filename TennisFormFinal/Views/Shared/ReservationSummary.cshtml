@model TennisFormFinal.Models.TennisReservation 
<tr>
    <td>
        @Html.DisplayFor(modelItem => Model.Court.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.ReservationTime)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.MatchType)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.Court.Type)
    </td>
    <td>
        @Model.GetPrice()
    </td>

    <td>
        <a asp-action="Delete" asp-route-id="@Model.ReservationId">Delete</a>
    </td>
</tr>
